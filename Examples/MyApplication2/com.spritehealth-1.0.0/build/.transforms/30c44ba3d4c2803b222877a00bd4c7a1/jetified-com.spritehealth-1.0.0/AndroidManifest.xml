<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.spritehealth.sdk"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="29"
        android:targetSdkVersion="30" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.MODIFY_AUDIO_SETTINGS" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true" >
        <activity
            android:name="com.spritehealth.sdk.VPTFinder"
            android:label="@string/title_activity_v_p_t_finder" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.spritehealth.sdk.SpecialistDetail"
            android:label="@string/title_activity_specialist_detail" >
        </activity>
        <activity
            android:name="com.spritehealth.sdk.BookAppointment"
            android:label="Select Date &amp; Time" >
        </activity>
        <activity
            android:name="com.spritehealth.sdk.PreviewAppointment"
            android:label="Review Appointment" >
        </activity>
        <activity
            android:name="com.spritehealth.sdk.AppointmentDetails"
            android:label="Appointment Details" >
        </activity>
    </application>

</manifest>