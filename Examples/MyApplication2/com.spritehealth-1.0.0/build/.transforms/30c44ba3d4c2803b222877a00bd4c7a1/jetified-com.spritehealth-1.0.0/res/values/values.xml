    <style name="CustomEditTextStyle" parent="android:Widget.EditText">
        <item name="android:textColor">@color/red</item>
    </style>
    <style name="Theme.SDK" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/white</item>
        <item name="colorPrimaryVariant">@color/white</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/black</item>
        <item name="colorSecondaryVariant">@color/black</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" ns1:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
    <style name="Theme.SDK.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar"/>
    <style name="Theme.SDK.Button">
        <item name="android:background">@drawable/wp_button_bg</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_marginTop">10dp</item>
    </style>
    <style name="Theme.SDK.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="Theme.SDK.PageHeading">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:textSize">20sp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:background">@drawable/border_bottom</item>
        <item name="android:layout_alignParentTop">true</item>
        <item name="android:orientation">horizontal</item>
    </style>
    <style name="Theme.SDK.PopupOverlay" parent="ThemeOverlay.AppCompat.Light"/>
    <style name="Theme.SDK.Section">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">#ffffff</item>
    </style>
    <style name="Theme.SDK.Section.Field">
        <item name="android:paddingLeft">5dp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:typeface">sans</item>
        <item name="android:textSize">12sp</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:layout_marginTop">5dp</item>
    </style>
    <style name="Theme.SDK.Section.Field.Label">
        <item name="android:textStyle">bold</item>
    </style>
    <style name="Theme.SDK.Section.Heading">
        <item name="android:background">#ffffff</item>
        <item name="android:padding">5dp</item>
    </style>
    <style name="Theme.SDK.SectionHeading">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:textSize">18sp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_alignParentTop">true</item>
        <item name="android:orientation">horizontal</item>
    </style>
    <style name="wpBackground">
        <item name="android:background">@color/lightGray</item>
    </style>
    <style name="wpBlockRounded">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/wp_box_shadow</item>
        <item name="android:paddingLeft">2dp</item>
        <item name="android:paddingRight">2dp</item>
        <item name="android:paddingTop">2dp</item>
        <item name="android:paddingBottom">2dp</item>
    </style>
    <style name="wpBody1" parent="wpTypeface">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:alpha">0.87</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="wpBody2" parent="wpTypeface">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:alpha">0.87</item>
    </style>
    <style name="wpBody3" parent="wpTypeface">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:alpha">0.87</item>
    </style>
    <style name="wpButton" parent="wpCommonStyle">
        <item name="android:background">@drawable/border_top_touch</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">4dp</item>
        <item name="android:textSize">18sp</item>
        <item name="android:layout_marginLeft">0dp</item>
        <item name="android:layout_marginRight">0dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/primary</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="wpCommonStyle">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/black</item>
    </style>
    <style name="wpEditText">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textAppearance">@style/wpBody2</item>
        <item name="android:background">@drawable/wp_edittext</item>
    </style>
    <style name="wpEditTextTransparent">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textAppearance">@style/wpBody2</item>
        <item name="android:background">@drawable/wp_edittext_transparent</item>
    </style>
    <style name="wpErrorBody" parent="wpTypeface">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/red</item>
        
